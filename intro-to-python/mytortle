import turtle
import turtle
#square
turtle.color("red")
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)

#blackwidow
turtle.fillcolor("orange")
turtle.up()
turtle.forward(200)
turtle.down()
turtle.left(45)
turtle.forward(200)
turtle.right(135)
turtle.forward(200)
turtle.right(135)
turtle.forward(200)
turtle.left(135)
turtle.forward(85)
